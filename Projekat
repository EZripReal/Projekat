def kviz(pitanja):
	rezultat=0
	brojevi=list()
	for i in range(100):
		brojevi.append(i)
	random.shuffle(brojevi)
	for i in range(10):
		pitanje, odgovor=lista_pitanja[brojevi[i]]
		print(pitanje)
		odgovori=input()

		if odgovor==odgovori:
			rezultat+=1
	print('Vas rezultat:'+ str(rezultat)+' od 10')
kviz(lista_pitanja)
